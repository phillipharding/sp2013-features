                  Image32by32="/_layouts/15/$Resources:core,Language;/images/formatmap32x32.png?rev=23" Image32by32Top="-171" Image32by32Left="-511"
                  LabelText="$Resources:core,ButEditLink;"
                  Sequence="5" 
