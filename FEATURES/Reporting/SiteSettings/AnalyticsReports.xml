				<CommandUIDefinition Location="Ribbon.Documents.Share.Controls._children"> <Button Id="Ribbon.Documents.Share.ViewAnalyticsReport" Sequence="55" Command="viewAnalyticsReport" Image16by16="/_layouts/15/images/analyticsreport16x16.png?rev=23" Image32by32="/_layouts/15/images/analyticsreport32x32.png?rev=23" LabelText="$Resources:dlccore,Reporting_AnalyticsReportingFeatureTitle;" ToolTipTitle="$Resources:dlccore,Reporting_AnalyticsReportingFeatureTitle;" ToolTipDescription="$Resources:dlccore,SiteSettings_AnalyticsReports_Tooltip;" TemplateAlias="o1"/> </CommandUIDefinition>
