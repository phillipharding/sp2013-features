        </VersionRange>
        <VersionRange EndVersion="15.0.13.0">
            <CustomUpgradeAction Name="DisableSaveSiteAsTemplate" />
        </VersionRange>
        <VersionRange EndVersion="15.0.14.0">
          <ApplyElementManifests>
            <ElementManifest Location="ProvisionedUI3.xml" />
          </ApplyElementManifests>
        </VersionRange>
    </UpgradeActions>
