                <MAPPING Value="3">$Resources:spscore,SiteMembership_Status_Inactive;</MAPPING>
                <MAPPING Value="4">$Resources:spscore,SiteMembership_Status_Blocked;</MAPPING>
                <MAPPING Value="5">$Resources:spscore,SiteMembership_Status_Left;</MAPPING>
            </MAPPINGS>
