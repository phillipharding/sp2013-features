        <ElementManifest Location="ListTemplates\MicroBlogList.xml"/>
        <ElementManifest Location="MicroBlogList\MicroBlogListData.xml"/>
    </ElementManifests>

    <UpgradeActions>
        <VersionRange EndVersion="15.0.1.0">
