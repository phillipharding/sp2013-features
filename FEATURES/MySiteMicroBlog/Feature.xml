             <CustomUpgradeAction Name="UpdateImageAndPreventListDeletion" />
        </VersionRange>
        <VersionRange EndVersion="15.0.3.0">
            <CustomUpgradeAction Name="UpdateFeedListSchema" />
        </VersionRange>
    </UpgradeActions>
</Feature>
